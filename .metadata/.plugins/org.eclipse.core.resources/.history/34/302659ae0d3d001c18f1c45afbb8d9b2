package inference;

public class Customer {
	private Person person;
	private Person emp;

	public Customer(Person emp) {};
	
	public Person getPerson() {
		return person;
	}

	public void setPerson(Person person) {
		this.person = person;
	}

	public Person getEmp() {
		return emp;
	}

	public void setEmp(Person employee) {
		this.emp = emp;
	}
	
}
